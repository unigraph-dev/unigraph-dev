{
    "displayName": "Todo",
    "version": "0.2.2",
    "description": "Todo lists and task management.",
    "name": "unigraph.todo",
    "unigraph": {
        "schemas": [
            {
                "id": "todo",
                "src": "schemas/todo.json"
            }
        ],
        "entities": [
            {
                "id": "add_follow_up_context_menu",
                "src": "entities/add_follow_up_context_menu.json"
            },
            {
                "id": "todo_view",
                "src": "entities/todo_view.json"
            },
            {
                "id": "todo_archived_tags",
                "src": "entities/todo_archived_tags.json"
            }
        ],
        "executables": [
            {
                "id": "add-follow-up-todo",
                "env": "routine/js",
                "src": "executables/addFollowUpContextMenu.js",
                "editable": true,
                "name": "Add an item as follow-up todo task"
            }
        ]
    }
}
