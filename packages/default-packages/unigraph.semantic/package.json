{
    "displayName": "Semantic",
    "version": "0.3.0",
    "description": "Semantic properties support for unigraph objects.",
    "name": "unigraph.semantic",
    "unigraph": {
        "schemas": [
            {
                "id": "color",
                "src": "schemas/color.json"
            },
            {
                "id": "regex",
                "src": "schemas/regex.json"
            },
            {
                "id": "subentity",
                "src": "schemas/subentity.json"
            },
            {
                "id": "person",
                "src": "schemas/person.json"
            },
            {
                "id": "tag",
                "src": "schemas/tag.json"
            },
            {
                "id": "note",
                "src": "schemas/note.json"
            },
            {
                "id": "html",
                "src": "schemas/html.json"
            },
            {
                "id": "markdown",
                "src": "schemas/markdown.json"
            },
            {
                "id": "interface/textual",
                "src": "schemas/textual.json"
            },
            {
                "id": "interface/semantic",
                "src": "schemas/semantic.json"
            },
            {
                "id": "semantic_property",
                "src": "schemas/semantic_property.json"
            },
            {
                "id": "creative_work",
                "src": "schemas/creative_work.json"
            },
            {
                "id": "message",
                "src": "schemas/message.json"
            },
            {
                "id": "icon_url",
                "src": "schemas/icon_url.json"
            },
            {
                "id": "url",
                "src": "schemas/url.json"
            }
        ],
        "executables": [
            {
                "id": "add-semantic-property",
                "env": "routine/js",
                "src": "executables/addSemanticProperty.js",
                "editable": true,
                "name": "Adds a semantic property to an object"
            },
            {
                "id": "rename-entity",
                "env": "routine/js",
                "src": "executables/renameEntity.js",
                "editable": true,
                "name": "Renames an entity and update all its references"
            }
        ]
    }
}
