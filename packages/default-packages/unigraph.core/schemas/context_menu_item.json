{
    "dgraph.type": "Type",
    "_name": "Context menu item",
    "_definition": {
        "type": { "unigraph.id": "$/composer/Object" },
        "_parameters": {
            "_indexedBy": { "unigraph.id": "$/primitive/string" }
        },
        "_properties": [
            {
                "_key": "name",
                "_definition": {
                    "type": { "unigraph.id": "$/primitive/string" }
                },
                "_indexAs": "name"
            },
            {
                "_key": "children",
                "_definition": {
                    "type": {
                        "unigraph.id": "$/composer/Array"
                    },
                    "_parameters": {
                        "_element": {
                            "type": { "unigraph.id": "$/composer/Union" },
                            "_parameters": {
                                "_definitions": [
                                    {
                                        "type": { "unigraph.id": "$/schema/subentity" }
                                    },
                                    {
                                        "type": { "unigraph.id": "$/schema/interface/semantic" }
                                    }
                                ]
                            }
                        }
                    }
                }
            },
            {
                "_key": "view",
                "_definition": {
                    "type": { "unigraph.id": "$/schema/view" }
                }
            },
            {
                "_key": "on_click",
                "_definition": {
                    "type": { "unigraph.id": "$/schema/executable" }
                }
            },
            {
                "_key": "icon",
                "_definition": {
                    "type": { "unigraph.id": "$/schema/icon_url" }
                }
            },
            {
                "_key": "is_shortcut",
                "_definition": {
                    "type": { "unigraph.id": "$/primitive/boolean" }
                }
            },
            {
                "_key": "item_type",
                "_definition": {
                    "type": { "unigraph.id": "$/schema/any" }
                }
            }
        ]
    }
}
