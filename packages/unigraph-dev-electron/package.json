{
    "name": "unigraph-dev-electron",
    "version": "0.1.0",
    "author": "Unigraph contributors",
    "description": "A local-first and universal graph database, knowledge engine, and workspace",
    "main": "src/main.js",
    "scripts": {
        "start": "electron . --dev",
        "dist": "electron-builder"
    },
    "homepage": "./",
    "devDependencies": {
        "electron": "13.1.7",
        "electron-builder": "^22.10.5"
    },
    "dependencies": {
        "electron-store": "^8.0.0",
        "electron-util": "^0.15.1",
        "@babel/core": "^7.14.8",
        "@babel/preset-react": "^7.14.5",
        "json-stable-stringify": "^1.0.1",
        "unigraph-dev-backend": "0.1.0",
        "unigraph-dev-common": "0.0.1"
    },
    "electronVersion": "^13.1.7",
    "build": {
        "files": [
            "**/*",
            "*.js",
            "buildweb",
            "dgraph",
            "distnode",
            "src",
            "node_modules/**/*",
            "!p/",
            "!w/",
            "!zw/"
        ],
        "asar": false,
        "asarUnpack": [
            "dgraph/*"
        ]
    }
}
